#include <iostream> 
#include <iostream>
using namespace std;

int main()
{
 int i, j;
 for(i=1; i<=5; i++)
 {
  for(j=1; j<=i; j++)
   cout << (i+j-1) << " ";
  cout << "\n";
 }
 return 0;
    int rows, num = 1;
    cout << "Enter number of rows: ";
    cin >> rows;

    for(int i = 1; i <= rows; i++)
   {
        for(int j = 1; j <= i; j++)
        {
            cout << num*num*num << " ";
            num++;
        }
        cout << endl;
    }

    return 0;
}
